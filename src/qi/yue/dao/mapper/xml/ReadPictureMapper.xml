<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="temp.mapper.TReadPictureMapper">
	<resultMap id="BaseResultMap" type="temp.po.TReadPicture">
		<id column="id" property="id" jdbcType="INTEGER" />
		<result column="uid" property="uid" jdbcType="INTEGER" />
		<result column="latitude" property="latitude" jdbcType="DECIMAL" />
		<result column="longitude" property="longitude" jdbcType="DECIMAL" />
		<result column="create_at" property="createAt" jdbcType="TIMESTAMP" />
		<result column="update_at" property="updateAt" jdbcType="TIMESTAMP" />

		<result column="msg" property="msg" jdbcType="LONGVARCHAR" />
	</resultMap>

	<insert id="insert" parameterType="temp.po.TReadPicture">
		insert into t_read_picture
		(id, uid, latitude,
		longitude, create_at, update_at,
		msg)
		values
		(#{id,jdbcType=INTEGER}, #{uid,jdbcType=INTEGER},
		#{latitude,jdbcType=DECIMAL},
		#{longitude,jdbcType=DECIMAL},
		#{createAt,jdbcType=TIMESTAMP}, #{updateAt,jdbcType=TIMESTAMP},
		#{msg,jdbcType=LONGVARCHAR})
	</insert>
</mapper>