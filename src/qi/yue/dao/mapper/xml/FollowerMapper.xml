<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="qi.yue.dao.mapper.FollowerMapper">
	<resultMap id="BaseResultMap" type="qi.yue.entity.Follower">
		<id column="fid" property="fid" jdbcType="INTEGER" />
		<result column="tid" property="tid" jdbcType="INTEGER" />
		<result column="face_url" property="faceUrl" jdbcType="VARCHAR" />
		<result column="signature" property="signature" jdbcType="VARCHAR" />
		<result column="nickname" property="nickname" jdbcType="VARCHAR" />
	</resultMap>
	<sql id="Base_Column_List">
		fid, tid, face_url, signature, nickname
	</sql>
	<select id="findByTid" resultMap="BaseResultMap" parameterType="java.lang.Integer">
		select
		<include refid="Base_Column_List" />
		from t_follower
		where tid = #{tid,jdbcType=INTEGER}
	</select>
</mapper>